{
  "name": "post",
  "plural": "posts",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "sid": {
      "type": "string",
      "id": true,
      "generated": false
    },
    "status": {
      "type": "string",
      "default": "pending"
    },
    "thumbnailUrl": {
      "type": "string"
    },
    "numOfImages": {
      "type": "number"
    },
    "message": {
      "type": "string"
    },
    "likes": {
      "type": "object"
    },
    "nodes": {
      "type": "object"
    },
    "address": {
      "type": "string"
    },
    "geolocation": {
      "type": "geopoint"
    },
    "created": {
      "type": "date"
    },
    "modified": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "nodes": {
      "type": "hasMany",
      "model": "nodemeta",
      "foreignKey": "postId"
    },
    "snapshots": {
      "type": "embedsMany",
      "model": "snapshot",
      "property": "snapshotList",
      "options": {
        "validate": true,
        "forceId": false
      }
    },
    "files": {
      "type": "hasMany",
      "model": "file",
      "foreignKey": "postId"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "accessType": "WRITE",
      "permission": "DENY"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "accessType": "WRITE",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
