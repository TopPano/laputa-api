{
  "name": "nodemeta",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "sid": {
      "type": "string",
      "id": true,
      "generated": false,
      "defaultFn": "uuid"
    },
    "tag": {
      "type": "string"
    },
    "heading": {
      "type": "number"
    },
    "enabled": {
      "type": "boolean"
    },
    "width": {
      "type": "number"
    },
    "height": {
      "type": "number"
    },
    "thumbnailUrl": {
      "type": "string"
    },
    "srcUrl": {
      "type": "string"
    },
    "srcDownloadUrl": {
      "type": "string"
    },
    "transitions": {
      "type": [
        {
          "lat": "number",
          "lng": "number",
          "size": "number",
          "nextNodeId": "string"
        }
      ]
    }
  },
  "validations": [],
  "relations": {
    "model": {
      "type": "belongsTo",
      "model": "modelmeta",
      "foreignKey": "modelId"
    },
    "files": {
      "type": "hasMany",
      "model": "file",
      "foreignKey": "nodeId"
    }
  },
  "acls": [],
  "methods": {}
}
