{
  "name": "modelmeta",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "sid": {
      "type": "string",
      "id": true,
      "generated": false,
      "defaultFn": "uuid"
    },
    "name": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "image": {
      "type": "string"
    },
    "nodes": {
      "type": "object"
    },
    "viewerOptions": {
      "type": {
        "description": "boolean",
        "map": "boolean",
        "share": "boolean",
        "presentedBy": "boolean"
      }
    },
    "presentedBy": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "geolocation": {
      "type": "geopoint"
    },
    "description": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "nodes": {
      "type": "hasMany",
      "model": "nodemeta",
      "foreignKey": "modelId"
    },
    "snapshots": {
      "type": "embedsMany",
      "model": "snapshot",
      "property": "snapshotList",
      "options": {
        "validate": true,
        "forceId": false
      }
    },
    "sharedModels": {
      "type": "hasMany",
      "model": "modelshare",
      "foreignKey": "modelId"
    },
    "files": {
      "type": "hasMany",
      "model": "file",
      "foreignKey": "modelId"
    },
    "labels": {
      "type": "embedsMany",
      "model": "label",
      "property": "labelList",
      "options": {
        "validate": true,
        "forceId": false
      }
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "accessType": "WRITE",
      "permission": "DENY"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "accessType": "WRITE",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
